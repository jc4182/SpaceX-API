  "homepage": "https://github.com/r-spacex/SpaceX-API",
  "dependencies": {
    "blake3": "^2.1.7",
    "casbin": "^5.15.0",
    "cheerio": "^1.0.0-rc.10",
    "cron": "^1.8.3",
    "fuzzball": "^2.1.2",
    "got": "^12.0.4",
    "ioredis": "^5.0.4",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
